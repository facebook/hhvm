  This template defines a RegisterStructs function for registering
  all program structs using a provided 'register' callback.

}}
// RegisterTypes registers types found in this file that have a thrift_uri with the passed in registry.
func RegisterTypes(registry interface {
	  RegisterType(name string, initializer func() any)
}) {
    {{#program:structs}}
    {{#struct:thrift_uri}}
    registry.RegisterType("{{struct:thrift_uri}}", func() any { return {{struct:go_qualified_new_func}}() })
    {{/struct:thrift_uri}}
    {{/program:structs}}

    {{#program:enums}}
    {{#enum:thrift_uri}}
    registry.RegisterType("{{enum:thrift_uri}}", func() any { return {{enum:go_name}}(0) })
    {{/enum:thrift_uri}}
    {{/program:enums}}
}
