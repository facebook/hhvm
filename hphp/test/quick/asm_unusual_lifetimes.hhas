# This input used to cause the HHIR simplifier to do a use-after-free in the IR
# NSame opcode.

.function foo($a, $b) {
  CGetL $a
  Dup
  CGetL $b
  UnsetL $a
  Same
  SetL $k
  PopC

  CGetL $b
  UnsetL $b
  Same
  Not
  SetL $k2
  PopC

  CGetL $k
  CGetL $k2
  Add
  RetC
}

.function ["__EntryPoint"("""y:0:{}""")] main() {
  Int 1
  Int 2
  Int 3
  NewVec 3
  SetL $a
  PopC
  Int 1
  Int 2
  Int 3
  NewVec 3
  SetL $b
  PopC
  NullUninit
  NullUninit
    PushL $a
    PushL $b
  FCallFuncD <> 2 1 "" - "" "foo"
  RetC
}
