# Test making closures

.class [no_override] Closure$Test extends Closure {
  .property [private] N x = uninit;
  .method [public static] N __invoke() isClosureBody {
    .declvars $x;
    CGetL $x
    RetC
  }
}

.function ["__EntryPoint"("""y:0:{}""")] N main {
  .declvars $x;
  Int 20
  SetL $x
  PopC
  NullUninit
  NullUninit
  CUGetL $x
  CreateCl 1 "Closure$Test"

  FCallFunc <> 0 1 "" "" - ""
  Print
  PopC

  Int 1
  RetC
}
