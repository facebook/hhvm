  True
  Mul
  FPassCE 0
  FCall 1
  PopR

# HHIRSimplification == false to test two immediates

  FPushFuncD 1 "print_bool_result"
  True
  True
  Add
  FPassCE 0
  FCall 1
  PopR

  FPushFuncD 1 "print_bool_result"
  True
  False
  Sub
  FPassCE 0
  FCall 1
  PopR

  FPushFuncD 1 "print_bool_result"
  False
  False
  BitOr
  FPassCE 0
  FCall 1
  PopR

  FPushFuncD 1 "print_bool_result"
  False
  True
  BitAnd
  FPassCE 0
  FCall 1
  PopR

  FPushFuncD 1 "print_bool_result"
  True
  False
  BitXor
  FPassCE 0
  FCall 1
  PopR

  FPushFuncD 1 "print_bool_result"
  False
  True
  Mul
  FPassCE 0
  FCall 1
  PopR

  Null
  RetC
}
