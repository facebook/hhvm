.hh_file 1;
#tests that files with negative IVA values fail assembly

.main {
  .declvars $foo $baz $t;
  Null
  SetL $foo
  PopC
  String "abc"
  SetL $baz
  PopC
  String "xyz"
  SetL $t
  PopC
  BaseL $baz Warn
  QueryM 0 CGet EL:$t
  Int 12
  BaseL $foo Define
  Dim Define ET:"x"
  Dim Define ET:"y"
  Dim Define PT:"bar"
  Dim Define ET:"q"
  SetM -1 PC:1
  PopC
  NullUninit
  NullUninit
  NullUninit
  CGetL $foo
  FCallFuncD <> 1 1 "" - "" "var_dump"
  PopC
  Int 1
  RetC
}
