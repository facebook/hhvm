.hh_file 1;
# Test that B *doesn't* get marked AlwaysHoistable and so
# DefClsNop won't actually define it

.main {
  DefClsNop 0
  DefClsNop 1

  NullUninit
  NullUninit
  NullUninit
  FCallClsMethodD <> 0 1 "" - "" "B" "print"
  PopC

  Int 1
  RetC
}

.class [unique] B extends A {
  .default_ctor;

  .method [public static] print() {
    String "hi\n"
    Print
    PopC

    Null
    RetC
  }

}

.class [unique] A {
  .default_ctor;
}
