  UnboxR
  FPushFuncD 0 "g_false"
  FCall 0
  UnboxR
  BitXor
  FPassCE 0
  FCall 1
  PopR

  FPushFuncD 1 "print_bool_result"
  FPushFuncD 0 "g_false"
  FCall 0
  UnboxR
  FPushFuncD 0 "g_true"
  FCall 0
  UnboxR
  Mul
  FPassCE 0
  FCall 1
  PopR

  FPushFuncD 1 "print_bool_result"
  FPushFuncD 0 "g_true"
  FCall 0
