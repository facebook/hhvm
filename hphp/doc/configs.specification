- bool Eval.LoadFilepathFromUnitCache = false, UNKNOWN, unitcacheflag

- bool Eval.WarnOnSkipFrameLookup = true, UNKNOWN

- uint32_t Eval.EnableCodeCoverage = 0, UNKNOWN

