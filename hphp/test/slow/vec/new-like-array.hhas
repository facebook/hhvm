#
# NewLikeArray on hack arrays should create a PHP array
#

.main {
  NullUninit
  NullUninit
  NullUninit
  FPushFuncD 0 "main"
  FCall <> 0 1 "" - "" ""
  PopC
  Int 1
  RetC
}

.function main {
          Int 1
          Int 2
          Int 3
          NewVecArray 3
          SetL $v
          PopC
          NullUninit
          NullUninit
          NullUninit
          NewLikeArrayL $v 3
          FPushFuncD 1 "var_dump"
          FCall <> 1 1 "" - "" ""
          PopC
          True
          RetC
}
