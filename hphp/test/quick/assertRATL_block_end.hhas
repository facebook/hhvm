# tests that AssertRATL cannot appear at the end of a block

.main {
    FPushFuncD 1 "foo"
    Int 9
    FCall <> 1 1 "" - "" ""
    PopC
    Int 1
    RetC
}

.function foo($x){
    Int 2
    JmpNZ target_label
    AssertRATL $x Int
    target_label: FPushFuncD 1 "var_dump"
    Int 42
    FCall <> 1 1 "" - "" ""
    RetC
}
