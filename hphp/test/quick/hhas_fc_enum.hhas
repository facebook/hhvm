# Test out making enums

.main {
  DefCls 0
  FPushFuncD 1 "test"
  ClsCnsD "FOO" "Foo"
  FCall <> 1 1 "" - "" ""
  PopC
  FPushFuncD 1 "test"
  String "hi"
  FCall <> 1 1 "" - "" ""
  PopC
  Int 1
  RetC
}

.function test(<"Foo" "Foo" hh_type> $x) {
  VerifyParamType $x
  Null
  RetC
}

.class [enum final] Foo extends HH\BuiltinEnum {
  .enum_ty <"HH\\int" hh_type extended_hint>;

  .const FOO = """i:1;""";
  .default_ctor;
}
