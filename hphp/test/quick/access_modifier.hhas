.hh_file 1;
#tests that fields and methods with != 1 access modifier fail verification

.main {
  .declvars $x;
  DefCls 0
  NewObjD "A"
  Dup
  NullUninit
  NullUninit
  String "foo"
  FCallCtor <LockWhileUnwinding> 1 1 "" - "" ""
  PopC
  LockObj
  SetL $x
  PopC
  CGetL $x
  NullUninit
  NullUninit
  String "bar"
  FCallObjMethodD <> 1 1 "" - "" "" NullThrows "set"
  PopC
  NullUninit
  NullUninit
  NullUninit
  CGetL $x
  NullUninit
  NullUninit
  FCallObjMethodD <> 0 1 "" - "" "" NullThrows "get"
  FCallFuncD <> 1 1 "" - "" "var_dump"
  PopC
  Int 1
  RetC
}

.class A {
  .property [public private] attre =
    """N;""";
  .property blah =
    """N;""";
  .method [protected public] <"" N  > get() {
    CheckThis
    BaseH
    QueryM 0 CGet PT:"attre"
    RetC
  }
  .method <"" N  > set($x) {
    CheckThis
    CGetL $x
    BaseH
    SetM 0 PT:"attre"
    PopC
    Null
    RetC
  }
  .method [public] <"" N  > __construct($attre) {
    CGetL $attre
    BaseH
    SetM 0 PT:"attre"
    PopC
    Null
    RetC
  }
}
