#
# Test some function call junk
#

.function caller($who, $arg) {
  NullUninit
  NullUninit
  NullUninit
  CGetL $arg
  CGetL $who
  FCallFunc <> 1 1 "" - ""

  SetL $ret
  PopC

  NullUninit
  NullUninit
  NullUninit
  CGetL $ret
  FCallFunc <> 0 1 "" - ""
  PopC

  True
  RetC
}

.function b() {
  String "In b() now\n"
  Print
  PopC

  True
  RetC
}

.function a($arg) {
  CGetL $arg
  String "\n"
  Concat
  Print
  PopC

  CGetL $arg
  RetC
}

.function byref(inout $foo) {
  Null
  Int 42
  SetL $foo
  RetM 2
}

.function ["__EntryPoint"("""y:0:{}""")] main() {
  NullUninit
  NullUninit
  NullUninit
  String "a"
  String "b"
  FCallFuncD <> 2 1 "" - "" "caller"
  PopC

  String "strval"
  SetL $funk
  PopC
  NullUninit
  NullUninit
  NullUninit
  NullUninit
  CGetL $funk
  Null
  PopL $funk
  FCallFuncD <> 1 2 "1" - "" "byref"
  PopC
  Print
  PopC
  String "\n"
  Print
  PopC

  True
  RetC
}
