# just testing istype opcodes

.function ["__EntryPoint"("""y:0:{}""")] N main() {
  .declvars $x;
  Int 1
  IsTypeC Int
  JmpNZ was_int

  String "Bad\n"
  Print
  PopC

was_int:
  String "ok\n"
  Print
  PopC

  Int 1
  SetL $x
  PopC

  IsTypeL $x Int
  JmpNZ was_int2

  String "bad2\n"
  Print
  RetC

was_int2:
  String "ok\n"
  Print
  PopC

  Int 1
  RetC
}
