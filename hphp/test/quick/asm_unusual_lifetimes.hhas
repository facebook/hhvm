# This input used to cause the HHIR simplifier to do a use-after-free in the IR
# NSame opcode.

.main {
  Int 1
  Int 2
  Int 3
  NewPackedArray 3
  SetL $a
  PopC
  Int 1
  Int 2
  Int 3
  NewPackedArray 3
  SetL $b
  PopC

  FPushFuncD 2 "main"
    PushL $a
    PushL $b
  FCall <> 2 1 "" - "" ""
  RetC
}

.function main($a, $b) {
  CGetL $a
  Dup
  CGetL $b
  UnsetL $a
  Same
  SetL $k
  PopC

  CGetL $b
  UnsetL $b
  Same
  Not
  SetL $k2
  PopC

  CGetL $k
  CGetL $k2
  Add
  RetC
}
