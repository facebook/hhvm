  test_naming_errors
  test_override
  test_remove_function
  test_remove_parent
  test_unsaved_changes)
 (libraries integration_test_base))

(rule
 (alias identify_symbol)
 (deps ../test_all.exe)
 (action
  (run ../test_all.exe ide/identify_symbol)))

