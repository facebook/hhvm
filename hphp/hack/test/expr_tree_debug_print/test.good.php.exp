<?hh
<<file: __EnableUnstableFeatures('expression_trees')>>

function f(): void {
  $x = ExampleDsl::makeTree(
    darray[
      "path" => __FILE__,
      "start_line" => 5,
      "end_line" => 5,
      "start_column" => 18,
      "end_column" => 19,
    ],
    darray[
      "splices" => dict[],
      "functions" => vec[],
      "static_methods" => vec[],
      "variables" => vec[],
      "lexically_enclosing_tree" => NULL,
    ],
    (
      (ExampleDsl $v) ==> {
        return $v->visitInt(
