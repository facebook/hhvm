#
# Basic test of continuations in HH assembler.
#

.function N R() isGenerator {
  CreateCont
  PopC
  Int 1
  Yield
  PopC
  Int 2
  Yield
  PopC
  Int 3
  Yield
  PopC
  Null
  RetC
}

.function ["__EntryPoint"("""y:0:{}""")] N main {
  .numiters 1;
  .declvars $i $0;
  NullUninit
  NullUninit
  FCallFuncD <> 0 1 "" "" - "" "R"
  PopL $0
  IterInit <> 0 $0 L0
  .try {
  L1:
    IterGetValue <> 0 $0
    PopL $i
    String "\n"
    CGetL2 $i
    Concat
    Print
    PopC
    IterNext <> 0 $0 L1
    Jmp L0
  } .catch {
    IterFree 0
    Throw
  }
L0:
  Int 1
  RetC
}

