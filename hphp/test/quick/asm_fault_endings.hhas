.main {
  .try {
    FPushFuncD 0 "main"
    FCall 0 0 1 - "" ""
    UnboxR
    RetC
  } .catch {
    PopC
    String "Finished!\n"
    Print
    RetC
  }
}

.function main {
  .try {
    String "foo\n"
    Print
    PopC

    .try {
      FPushCtorD 1 "Exception"
      String "ex"
      FCall 1 0 1 - "" ""
      PopR
      Throw
    } .catch {
      String "handler2\n"
      Print
      PopC
      Throw
    }
  } .catch {
    String "handler1\n"
    Print
    PopC
    Throw
  }
}
