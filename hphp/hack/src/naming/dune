(library
 (name elab_ffi)
 (modules)
 (wrapped false)
 (foreign_archives elab_ffi))

(rule
 (targets libelab_ffi.a)
 (deps
  (source_tree %{workspace_root}/hack/src))
 (locks /cargo)
 (action
  (run
   %{workspace_root}/hack/scripts/invoke_cargo.sh
   elab_ffi
   elab_ffi)))

