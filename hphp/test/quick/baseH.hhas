.hh_file 1;
# check that baseH fails to verify if $this can be null

.main {
  .declvars $foo $baz $t;
  Null
  SetL $foo
  PopC
  String "abc"
  SetL $baz
  PopC
  String "xyz"
  SetL $t
  PopC
  BaseL $baz Warn
  QueryM 0 CGet EL:$t
  Int 12
  BaseH
  Dim Define ET:"x"
  Dim Define ET:"y"
  Dim Define ET:"q"
  SetM 0 PC:1
  PopC
  NullUninit
  NullUninit
  NullUninit
  CGetL $foo
  FCallFuncD <> 1 1 "" - "" "var_dump"
  PopC
  RetC
}
