.strict 0;

.main {
  FPushFuncD 1 "var_dump"
  FPushFuncD 2 "add"
  String "1"
  String "2"
  FCall 2 0 1 "" ""
  UnboxR
  FCall 1 0 1 "" ""
  PopR
  Int 1
  RetC
}

.function <"" N  > add(<"HH\\int" "HH\\int"  > $a, <"HH\\int" "HH\\int"  > $b) {
  VerifyParamType $a
  VerifyParamType $b
