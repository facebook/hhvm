# Test making closures

.class [no_override unique] ClosureTest extends Closure {
  .property [private] x = uninit;
  .method [public static] __invoke() isClosureBody {
    .declvars $x;
    CGetL $x
    RetC
  }
}

.function ["__EntryPoint"("""y:0:{}""")] main {
  Int 20
  SetL $x
  PopC

  NullUninit
  NullUninit
  NullUninit

  CUGetL $x
  CreateCl 1 0

  FCallFunc <> 0 1 "" - ""
  Print
  PopC

  Int 1
  RetC
}
