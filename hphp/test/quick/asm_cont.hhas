#
# Basic test of continuations in HH assembler.
#

.main {
  .numiters 1;
  NullUninit
  NullUninit
  NullUninit
  FPushFuncD 0 "R"
  FCall <> 0 1 "" - "" ""
  IterInit 0 L0 $i
  .try {
  L1:
    String "\n"
    CGetL2 $i
    Concat
    Print
    PopC
    IterNext 0 L1 $i
    Jmp L0
  } .catch {
    IterFree 0
    Throw
  }
L0:
  Int 1
  RetC
}

.function R() isGenerator {
  CreateCont
  PopC
  Int 1
  Yield
  PopC
  Int 2
  Yield
  PopC
  Int 3
  Yield
  PopC
  Null
  RetC
}

