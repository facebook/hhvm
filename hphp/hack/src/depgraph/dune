(data_only_dirs cargo depgraph depgraph_writer)

(library
  (name depgraph)
  (modules)
  (wrapped false)
  (foreign_archives depgraph)
)

(rule
  (targets libdepgraph.a)
  (deps (source_tree %{workspace_root}/src))
  (locks /cargo)
  (action
    (run %{workspace_root}/scripts/build_rust_to_ocaml.sh
      depgraph
      depgraph
    )
  )
)
