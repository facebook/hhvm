#tests that verifier rejects illegal stack offsets in member keys

.function ["__EntryPoint"("""y:0:{}""")] N main() {
  .declvars $foo $baz $t;
  Null
  SetL $foo
  PopC
  String "abc"
  SetL $baz
  PopC
  String "xyz"
  SetL $t
  PopC
  BaseL $baz Warn Any
  QueryM 0 CGet EL:$t Any
  Int 12
  BaseL $foo Define Any
  Dim Define ET:"x" Any
  Dim Define ET:"y" Any
  Dim Define PT:"bar" Any
  Dim Define ET:"q" Any
  SetM 0 PC:2 Any
  PopC
  NullUninit
  NullUninit
  CGetL $foo
  FCallFuncD <> 1 1 "" "" - "" "var_dump"
  PopC
  RetC
}
