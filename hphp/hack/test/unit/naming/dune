(executable
    (name localmem_naming_tests)
    (modules localmem_naming_tests)
    (link_flags (:standard (:include ../../../src/dune_config/ld-opts.sexp)))
    (modes exe byte_complete)
    (libraries
        asserter
        client_ide_service
        disk
        errors
        global_options
        logging
        naming_provider
        provider_backend
        provider_context
        search_utils
        sys_utils
        tast_provider
