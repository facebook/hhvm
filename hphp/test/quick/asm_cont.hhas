#
# Basic test of continuations in HH assembler.
#

.main {
  .numiters 1;
  FPushFuncD 0 "R"
  FCall 0
  UnboxR
  IterInit 0 L0 $i
  .try_catch F2 {
  L1:
    String "\n"
    CGetL2 $i
    Concat
    Print
    PopC
    IterNext 0 L1 $i
  }
L0:
  Int 1
  RetC
F2:
  Catch
  IterFree 0
  Throw
}

.function R() isGenerator {
  CreateCont
  PopC
  Int 1
  Yield
  PopC
  Int 2
  Yield
  PopC
  Int 3
  Yield
  PopC
  Null
  RetC
}

