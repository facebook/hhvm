<?hh
abstract class FlobBase {
  public function f1(): void {
    throw new \Exception();
  }
}
final class Flobby extends \FlobBase {
  use \TFlobby;
}
trait TFlobby {
  private function asFlobby(): \Flobby {
    throw new \Exception();
  }
  final public function g(): void {
    $flobby = $this->asFlobby();
    $flobby->f1();
  }
}
