# setting strict types without php7.ScalarTypes flag = true should fail

.strict 1;

.main {
  FPushFuncD 1 "var_dump"
  FPushFuncD 2 "add"
  String "1"
  String "2"
  FCall <> 2 1 "" - "" ""
  FCall <> 1 1 "" - "" ""
  PopC
  Int 1
  RetC
}

.function <"HH\\int" "HH\\int" hh_type extended_hint > add(<"HH\\int" "HH\\int"  > $x, <"HH\\int" "HH\\int"  > $y) {
  VerifyParamType $x
  VerifyParamType $y
  CGetL $y
  CGetL2 $x
  AddO
  VerifyRetTypeC
  RetC
}
