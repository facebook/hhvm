*** Testing array_values() : usage variations ***

-- Iteration 1: int data --
array(4) {
  [0]=>
  int(0)
  [1]=>
  int(1)
  [2]=>
  int(12345)
  [3]=>
  int(-2345)
}

-- Iteration 2: float data --
array(5) {
  [0]=>
  float(10.5)
  [1]=>
  float(-10.5)
  [2]=>
  float(123456789000)
  [3]=>
  float(1.23456789E-9)
  [4]=>
  float(0.5)
}

-- Iteration 3: null data --
array(2) {
  [0]=>
  NULL
  [1]=>
  NULL
}

-- Iteration 4: bool data --
array(4) {
  [0]=>
  bool(true)
  [1]=>
  bool(false)
  [2]=>
  bool(true)
  [3]=>
  bool(false)
}

-- Iteration 5: empty string data --
array(2) {
  [0]=>
  string(0) ""
  [1]=>
  string(0) ""
}

-- Iteration 6: empty array data --
array(0) {
}

-- Iteration 7: string data --
array(3) {
  [0]=>
  string(6) "string"
  [1]=>
  string(6) "string"
  [2]=>
  string(11) "hello world"
}

-- Iteration 8: object data --
array(1) {
  [0]=>
  object(classA)#%d (0) {
  }
}

-- Iteration 9: undefined data --
array(1) {
  [0]=>
  NULL
}

-- Iteration 10: unset data --
array(1) {
  [0]=>
  NULL
}

-- Iteration 11: resource data --
array(1) {
  [0]=>
  resource(%d) of type (stream)
}
Done