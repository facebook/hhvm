*** Testing array_shift() : usage variations ***

-- Iteration 1: int data --
int(0)
vec(3) {
  int(1)
  int(12345)
  int(-2345)
}

-- Iteration 2: float data --
float(10.5)
vec(4) {
  float(-10.5)
  float(123456789000)
  float(1.23456789E-9)
  float(0.5)
}

-- Iteration 3: null data --
NULL
vec(1) {
  NULL
}

-- Iteration 4: bool data --
bool(true)
vec(3) {
  bool(false)
  bool(true)
  bool(false)
}

-- Iteration 5: empty string data --
string(0) ""
vec(1) {
  string(0) ""
}

-- Iteration 6: empty array data --
NULL
vec(0) {
}

-- Iteration 7: string data --
string(6) "string"
vec(2) {
  string(6) "string"
  string(11) "hello world"
}

-- Iteration 8: object data --
object(classA) (0) {
}
vec(0) {
}

-- Iteration 9: resource data --
resource(4) of type (stream)
vec(0) {
}
