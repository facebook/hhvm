function update_value(vec<int> $items): vec<int> {
  // Both of these operations set $items a modified copy of the
  // original value.
  $items[0] = 42;
  $items[] = 100;
  return $items;
}

function demo(): void {
  $v = vec[1, 2];
  // $v is unaffected by this function call.
  $v2 = update_value($v);

  var_dump($v); // vec[1, 2]
  var_dump($v2); // vec[42, 2, 100]
}
