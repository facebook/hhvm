{{#import "common/constants" as constants}}
{{#import "common/type" as types}}
{{#import "types/field" as m_field}}
{{#import "svc/client" as m_client}}
{{#import "svc/handler" as m_handler}}
{{#import "svc/processor" as m_proc}}
{{> common/auto_generated_go}}

{{#if root_program.docs?}}
{{root_program.docs}}
{{/if root_program.docs?}}
package {{root_program.go_pkg_name}}

import (
    "context"
    "errors"
    "fmt"
    "io"
    "iter"
    "reflect"
    "sync"

    {{#each root_program.thrift_imports as |program|}}
    {{program.go_package_alias}} "{{program.go_import_path}}"
    {{/each}}
    thrift "{{constants.thrift_lib_import}}"
    {{#if (and root_program.gen_metadata? root_program.import_metadata_package?)}}
    metadata "{{constants.thrift_metadata_import}}"
    {{/if (and root_program.gen_metadata? root_program.import_metadata_package?)}}
)

// (needed to ensure safety because of naive import list construction)
{{> common/unused_imports_protection}}
var _ = context.Background
var _ = errors.New
var _ = fmt.Printf
var _ = io.EOF
var _ = iter.Pull[bool]
var _ = reflect.Ptr
var _ = sync.OnceFunc
var _ = thrift.VOID
{{#if (and root_program.gen_metadata? root_program.import_metadata_package?)}}
var _ = metadata.GoUnusedProtection__
{{/if (and root_program.gen_metadata? root_program.import_metadata_package?)}}

{{#each root_program.interactions as |interaction|}}
{{#partial m_handler.handler_interface svc=interaction}}

{{#partial m_client.client svc=interaction}}

{{#partial m_proc.processor svc=interaction}}

{{/each}}

{{#each root_program.services as |service|}}
{{#partial m_handler.handler_interface svc=service}}

{{#partial m_client.client svc=service}}

{{#partial m_proc.processor svc=service}}

{{/each}}
