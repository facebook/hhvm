{{#import "common/functions" as m_functions}}

{{! A list of the argument and result types for functions on thrift services (and interactions) for the purposes of calling fill_specs on them }}
{{#let export partial arg_type_list |interface| captures |m_functions|}}
  {{#each interface.supported_functions as |func|}}
{{#partial m_functions.args_type service=interface function=func}},
    {{#if (not func.oneway?)}}
_fbthrift_{{interface.name}}_{{func.name}}_result,
    {{/if (not func.oneway?)}}
    {{#if func.bidirectional_stream?}}
_fbthrift_{{interface.name}}_{{func.name}}_result_stream_elem,
_fbthrift_{{interface.name}}_{{func.name}}_result_sink_elem,
    {{#else}}
      {{#if func.stream?}}
_fbthrift_{{interface.name}}_{{func.name}}_result_stream_elem,
      {{/if func.stream?}}
      {{#if func.sink?}}
_fbthrift_{{interface.name}}_{{func.name}}_result_sink_elem,
_fbthrift_{{interface.name}}_{{func.name}}_result_sink_final,
      {{/if func.sink?}}
    {{/if func.bidirectional_stream?}}
  {{/each}}
{{/let partial}}
