.hh_file 1;
#
# Some tests of property/constant initializers.
#

.main {
  NullUninit
  NullUninit
  NullUninit
  FPushFuncD 0 "init_test"
  FCall <> 0 1 "" - "" ""
  PopC

  NullUninit
  NullUninit
  NullUninit
  FPushFuncD 0 "uninit_test"
  FCall <> 0 1 "" - "" ""
  PopC

  Int 1
  RetC
}

.class Init {
  .default_ctor;
  .const BAR = """s:4:"1242";""";
  .property [static public] foo = """s:11:"static prop";""";
  .property [public] nonstat = """s:25:"this is a normal property";""";
}

.class Uninit {
  .default_ctor;
  .const BAR = uninit;
  .property [static public] foo = uninit;
  .property [public] nonstat = uninit;

  .method [public] 86cinit($which) {
    CGetL $which
    String "BAR"
    Same
    JmpZ wtf

    String "default BAR value"
    RetC

  wtf:
    String "unknown property"
    Fatal Runtime
  }

  .method [public] 86sinit() {
    String "default ::foo value"
    InitProp "foo" Static

    Null
    RetC
  }

  .method [public] 86pinit() {
    CheckProp "nonstat"
    JmpNZ isset
    String "code-created prop value"
    InitProp "nonstat" NonStatic

  isset:
    Null
    RetC
  }
}

.function do_test($who) {
  .numclsrefslots 1;

  CGetL $who
  ClsRefGetC 0
  ClsCns "BAR" 0
  String "\n"
  Concat
  Print
  PopC

  String "foo"
  CGetL $who
  ClsRefGetC 0
  CGetS 0
  String "\n"
  Concat
  Print
  PopC

  CGetL $who
  ClsRefGetC 0
  NewObj 0 NoGenerics
  Dup
  NullUninit
  NullUninit
  FCallCtor <> 0 1 "" - ""
  PopC
  LockObj
  SetL $obj
  PopC

  String "nonstat"
  BaseL $obj Warn
  QueryM 1 CGet PC:0
  String "\n"
  Concat
  Print
  PopC

  Null
  RetC
}

.function init_test {
  NullUninit
  NullUninit
  NullUninit
    String "Init"
  FPushFuncD 1 "do_test"
  FCall <> 1 1 "" - "" ""
  RetC
}

.function uninit_test {
  NullUninit
  NullUninit
  NullUninit
    String "Uninit"
  FPushFuncD 1 "do_test"
  FCall <> 1 1 "" - "" ""
  RetC
}
