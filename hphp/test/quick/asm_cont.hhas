.hh_file 1;
#
# Basic test of continuations in HH assembler.
#

.main {
  .numiters 1;
  NullUninit
  NullUninit
  NullUninit
  FCallFuncD <> 0 1 "" - "" "R"
  IterInit 0 NK V:$i L0
  .try {
  L1:
    String "\n"
    CGetL2 $i
    Concat
    Print
    PopC
    IterNext 0 NK V:$i L1
    Jmp L0
  } .catch {
    IterFree 0
    Throw
  }
L0:
  Int 1
  RetC
}

.function R() isGenerator {
  CreateCont
  PopC
  Int 1
  Yield
  PopC
  Int 2
  Yield
  PopC
  Int 3
  Yield
  PopC
  Null
  RetC
}

