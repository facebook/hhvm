.main {
  .try {
    FPushFuncD 0 "main"
    FCall <> 0 1 "" - "" ""
    RetC
  } .catch {
    PopC
    String "Finished!\n"
    Print
    RetC
  }
}

.function main {
  .try {
    String "foo\n"
    Print
    PopC

    .try {
      NewObjD "Exception"
      Dup
      FPushCtor 1
      String "ex"
      FCall <> 1 1 "" - "" ""
      PopC
      Throw
    } .catch {
      String "handler2\n"
      Print
      PopC
      Throw
    }
  } .catch {
    String "handler1\n"
    Print
    PopC
    Throw
  }
}
