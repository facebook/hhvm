# tests that constructors and destructors fail verification properly when
# poorly defined


.class A {
  .property [public] attre =
    """N;""";
  .method [public] <"" N  > get() {
    CheckThis
    BaseH
    QueryM 0 CGet PT:"attre"
    RetC
  }
  .method [public] <"" N  > set($x) {
    CheckThis
    CGetL $x
    BaseH
    SetM 0 PT:"attre"
    PopC
    Null
    RetC
  }
  .method [public static] <"" N  > __construct($attre) {
    CGetL $attre
    BaseH
    SetM 0 PT:"attre"
    PopC
    Null
    RetC
  }
}

.class B {
  .method [public] <"" N  > __construct() isClosureBody {
    String ""
    BaseH
    SetM 0 PT:"attre"
    PopC
    Null
    RetC
  }
}

.function ["__EntryPoint"("""y:0:{}""")] main() {
  .declvars $x;
  NewObjD "A"
  Dup
  NullUninit
  NullUninit
  String "foo"
  FCallCtor <LockWhileUnwinding> 1 1 "" - "" ""
  PopC
  LockObj
  SetL $x
  PopC
  CGetL $x
  NullUninit
  NullUninit
  String "bar"
  FCallObjMethodD <> 1 1 "" - "" "" NullThrows "set"
  PopC
  NullUninit
  NullUninit
  NullUninit
  CGetL $x
  NullUninit
  NullUninit
  FCallObjMethodD <> 0 1 "" - "" "" NullThrows "get"
  FCallFuncD <> 1 1 "" - "" "var_dump"
  PopC
  Int 1
  RetC
}
