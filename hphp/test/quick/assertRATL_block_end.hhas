.hh_file 1;
# tests that AssertRATL cannot appear at the end of a block

.main {
    NullUninit
    NullUninit
    NullUninit
    Int 9
    FCallFuncD <> 1 1 "" - "" "foo"
    PopC
    Int 1
    RetC
}

.function foo($x){
    NullUninit
    NullUninit
    NullUninit
    Int 2
    JmpNZ target_label
    AssertRATL $x Int
  target_label:
    Int 42
    FCallFuncD <> 1 1 "" - "" "var_dump"
    RetC
}
