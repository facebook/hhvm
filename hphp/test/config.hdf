Server {
  InjectedStacktrace = true
}

Eval {
  EnableXHP = true
  IncludePaths {
    * = .
    * = /usr/local/lib/php
    * = /usr/local/php/lib/php
  }
  AllowHhas = true
  EnableHipHopSyntax = true
  EnableObjDestructCall = true
}

Sandbox {
  MySQLReadTimeout = 5000
}

MySQL {
  ReadTimeout = 5000
}

EnvVariables {
  HPHP_INTERPRETER = 1
}

ServerVariables {
  ALPHA_CONSOLE = 1
  TFBENV = 16777216
}

ErrorHandling {
  NoticeFrequency = 1
  WarningFrequency = 1
}

ResourceLimit {
  SerializationSizeLimit=134217728
}

LintExcludePatterns {
  * = Server.InjectedStacktrace
  * = Eval.IncludePaths*
  * = Sandbox.MySQLReadTimeout
}
