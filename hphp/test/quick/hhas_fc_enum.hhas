.hh_file 1;
# Test out making enums

.main {
  DefCls 0
  NullUninit
  NullUninit
  NullUninit
  ClsCnsD "FOO" "Foo"
  FCallFuncD <> 1 1 "" - "" "test"
  PopC
  NullUninit
  NullUninit
  NullUninit
  String "hi"
  FCallFuncD <> 1 1 "" - "" "test"
  PopC
  Int 1
  RetC
}

.function test(<"Foo" "Foo"> $x) {
  VerifyParamType $x
  Null
  RetC
}

.class [enum final] Foo extends HH\BuiltinEnum {
  .enum_ty <"HH\\int" extended_hint>;

  .const FOO = """i:1;""";
  .default_ctor;
}
