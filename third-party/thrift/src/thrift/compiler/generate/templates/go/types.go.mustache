{{#import "common/constants" as constants}}
{{#import "common/type" as types}}
{{#import "codec/codec" as codec}}
{{#import "types/enum" as m_enum}}
{{#import "types/field" as m_field}}
{{#import "types/struct" as m_struct}}
{{#import "types/typedef" as m_typedef}}
{{> common/auto_generated_go}}

package {{root_program.go_pkg_name}}

import (
    "fmt"
    "reflect"

    {{#each root_program.thrift_imports as |program|}}
    {{program.go_package_alias}} "{{program.go_import_path}}"
    {{/each}}
    thrift "{{constants.thrift_lib_import}}"
    {{#if (and root_program.gen_metadata? root_program.import_metadata_package?)}}
    metadata "{{constants.thrift_metadata_import}}"
    {{/if (and root_program.gen_metadata? root_program.import_metadata_package?)}}
)

// (needed to ensure safety because of naive import list construction)
{{> common/unused_imports_protection}}
var _ = fmt.Printf
var _ = reflect.Ptr
var _ = thrift.VOID
{{#if (and root_program.gen_metadata? root_program.import_metadata_package?)}}
var _ = metadata.GoUnusedProtection__
{{/if (and root_program.gen_metadata? root_program.import_metadata_package?)}}

{{! Typedefs }}
{{#each root_program.typedefs as |t|}}
{{#partial m_typedef.typedef t=t}}

{{/each}}
{{! Enums }}
{{#each root_program.enums as |e|}}
{{#partial m_enum.enum e=e}}

{{/each}}
{{! Structs }}
{{#each root_program.structured_definitions as |s|}}
{{#partial m_struct.struct s=s}}

{{/each}}
{{! Req/resp structs }}
{{#each root_program.req_resp_structs as |s|}}
{{#partial m_struct.struct s=s}}

{{/each}}

func init() {
    {{#each root_program.structured_definitions as |struct|}}
    {{#if struct.uri}}
    thrift.InternalRegisterType("{{struct.uri}}", {{struct.codec_type_spec_name}})
    {{/if struct.uri}}
    {{/each}}
    {{#each root_program.typedefs as |typedef|}}
    {{#if typedef.uri}}
    thrift.InternalRegisterType("{{typedef.uri}}", {{typedef.codec_type_spec_name}})
    {{/if typedef.uri}}
    {{/each}}
    {{#each root_program.enums as |enum|}}
    {{#if enum.uri}}
    thrift.InternalRegisterType("{{enum.uri}}", {{enum.codec_type_spec_name}})
    {{/if enum.uri}}
    {{/each}}
}

{{#if root_program.structs?}}
// RegisterTypes registers types found in this file that have a thrift_uri with the passed in registry.
func RegisterTypes(registry interface {
  RegisterType(name string, initializer func() any)
}) {
    {{#each root_program.structured_definitions as |struct|}}
    {{#if struct.uri}}
    registry.RegisterType("{{struct.uri}}", func() any { return {{struct.go_qualified_new_func}}() })
    {{/if struct.uri}}
    {{/each}}

    {{#each root_program.enums as |enum|}}
    {{#if enum.uri}}
    registry.RegisterType("{{enum.uri}}", func() any { return {{enum.go_name}}(0) })
    {{/if enum.uri}}
    {{/each}}
}
{{/if root_program.structs?}}
