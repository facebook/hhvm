# exclude divide by zero

# try a few cases with one register and one immediate

  FPushFuncD 1 "print_bool_result"
  False
  FPushFuncD 0 "g_true"
  FCall 0
  UnboxR
  Add
  FPassCE 0
  FCall 1
  PopR

  FPushFuncD 1 "print_bool_result"
  FPushFuncD 0 "g_false"
