*** Testing array_slice() : usage variations ***

-- Iteration 1 : key type is int --
$preserve_keys = TRUE
darray(4) {
  [0]=>
  string(4) "zero"
  [1]=>
  string(3) "one"
  [12345]=>
  string(8) "positive"
  [-2345]=>
  string(8) "negative"
}
$preserve_keys = FALSE
darray(4) {
  [0]=>
  string(4) "zero"
  [1]=>
  string(3) "one"
  [2]=>
  string(8) "positive"
  [3]=>
  string(8) "negative"
}

-- Iteration 2 : key type is empty double quotes --
$preserve_keys = TRUE
darray(1) {
  [""]=>
  string(6) "emptyd"
}
$preserve_keys = FALSE
darray(1) {
  [""]=>
  string(6) "emptyd"
}

-- Iteration 3 : key type is empty single quotes --
$preserve_keys = TRUE
darray(1) {
  [""]=>
  string(6) "emptys"
}
$preserve_keys = FALSE
darray(1) {
  [""]=>
  string(6) "emptys"
}

-- Iteration 4 : key type is string --
$preserve_keys = TRUE
darray(3) {
  ["stringd"]=>
  string(7) "stringd"
  ["strings"]=>
  string(7) "strings"
  ["hello world"]=>
  string(7) "stringh"
}
$preserve_keys = FALSE
darray(3) {
  ["stringd"]=>
  string(7) "stringd"
  ["strings"]=>
  string(7) "strings"
  ["hello world"]=>
  string(7) "stringh"
}
