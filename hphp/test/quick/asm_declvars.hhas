.hh_file 1;
#
# test declvars
#

.main {
  NullUninit
  NullUninit
  NullUninit
  Int 12
  FCallFuncD <> 1 1 "" - "main"
  PopC
  Null
  RetC
}

.function main($x) {
  # $y should get assigned to _1 even though $z is used first
  .declvars $y $z;

  String "a"
  SetL $z
  PopC

  String "b"
  SetL $y
  PopC

  CGetL _1
  CGetL _2
  Concat
  Print
  PopC

  CGetL _0
  Print
  PopC

  Null
  RetC
}
