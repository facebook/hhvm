#
# Basic test of continuations in HH assembler.
#

.main {
  .numiters 1;
  FPushFuncD 0 "R"
  FCall 0
  UnboxR
  IterInit 0 L0 $i
  .try_fault F2 {
  L1:
    String "\n"
    CGetL2 $i
    Concat
    Print
    PopC
    IterNext 0 L1 $i
  }
L0:
  Int 1
  RetC
F2:
  IterFree 0
  Unwind
}

.function R() isGenerator {
  CreateCont L0
  RetC
L0:
  PopC
  Int 1
  ContSuspend
  PopC
  Int 2
  ContSuspend
  PopC
  Int 3
  ContSuspend
  PopC
  Null
  ContRetC
}

